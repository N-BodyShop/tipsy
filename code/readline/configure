cat > conftest.${ac_ext} <<EOF
#include "confdefs.h"
#include <sys/file.h>
EOF
# Some shells (Coherent) do redirections in the wrong order, so need
# the parens.
ac_err=`eval "($ac_cpp conftest.${ac_ext} >/dev/null) 2>&1"`
